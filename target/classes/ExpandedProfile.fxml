<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.canvas.Canvas?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.text.Text?>

<Pane fx:id="expandedProfilePane" prefHeight="350.0" prefWidth="250.0" visible="false" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="me.beastlymc.gameinsight.controller.ExpandedProfileController">
   <ImageView fx:id="expandedProfileImage" fitHeight="64.0" fitWidth="64.0" pickOnBounds="true" preserveRatio="true" />
   <Text fx:id="expandedProfileText" fill="WHITE" strokeType="OUTSIDE" strokeWidth="0.0" text="BeastlyMC956" />
   <Pane fx:id="accountTypePane" prefHeight="16.0">
      <Text fx:id="accountTypeText" layoutX="2.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Text" />
   </Pane>
   <Canvas fx:id="topCanvas" height="2.0" width="200.0" />
   <Pane fx:id="myAccountPane" layoutY="30.0" onMouseEntered="#onAccountHover" onMouseExited="#onAccountExit" prefHeight="30.0" prefWidth="0.0">
      <Text fx:id="myAccountText" layoutX="8.0" strokeType="OUTSIDE" strokeWidth="0.0" text="My Account" />
   </Pane>
   <Pane fx:id="settingsPane" onMouseEntered="#onSettingsHover" onMouseExited="#onSettingsExit" prefHeight="30.0" prefWidth="0.0">
      <Text fx:id="settingsText" layoutX="8.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Settings" />
   </Pane>
   <Canvas fx:id="bottomCanvas" height="2.0" width="200.0" />
   <Pane fx:id="logoutPane" onMouseClicked="#onLogoutClick" onMouseEntered="#onLogoutHover" onMouseExited="#onLogoutExit" prefHeight="30.0" prefWidth="0.0">
      <Text fx:id="logoutText" layoutX="8.0" strokeType="OUTSIDE" strokeWidth="0.0" text="Log Out" />
   </Pane>
   <Canvas fx:id="expandedProfileCanvas" height="200.0" width="2.0" />
</Pane>
